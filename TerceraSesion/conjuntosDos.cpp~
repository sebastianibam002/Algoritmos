#include <iostream>
#include <set>
using namespace std;

int main()
{
  set<int> myset;
  for(int i = 1; i <5; i++)
    {
      myset.insert(i *3);
    }

  for(int i = 0; i < 10; i++)
    {
      cout<<i;
      if(myset.count(i) != 0)
	{
	  cout<<"is an elemento of myset."<<endl;
	 
	}
      else
	{
	  cout<<"is not an element of myset."<<endl;
	}
    }


  return 0;
}
