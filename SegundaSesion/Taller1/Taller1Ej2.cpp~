#include <iostream>
#include <list>
#include <math.h>
using namespace std;

int main()
{
  list<unsigned int> listaPotencias;
  for(int i = 0; i <= 10; i++)
    {
      listaPotencias.push_back(3, i);
    }

  //imprimir potencias

  while(!listaPotencias.empty())
    {
      cout<<listaPotencias.front()<<endl;
      listaPotencias.pop_front()
      
    }
}
